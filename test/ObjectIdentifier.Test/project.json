{
    "version": "1.0.0-*",
    "testRunner": "nunit",
    "dependencies": {
        "dotnet-test-xunit": "2.2.0-preview2-build1029",
        "NUnit": "3.6.0",
        "NUnit.Runners": "3.6.0",
        "dotnet-test-nunit": "3.4.0-beta-3",
        "ObjectIdentifier": {
            "target": "project"
        }
    },
    "frameworks": {
        "netcoreapp1.0": {
            "dependencies": {
                "Microsoft.NETCore.App": {
                    "type": "platform",
                    "version": "1.0.1"
                }
            }
        }
    },
    "buildOptions": {
        "copyToOutput": {
            "include": ["xunit.runner.json"]
        }
    },
    "tooling": {
        "defaultNamespace": "ObjectIdentifier.Test"
    }
}